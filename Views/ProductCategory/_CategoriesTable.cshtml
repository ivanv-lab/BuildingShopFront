<table class="table table-hover" id="categoriesTable">
    <thead>
    <tr>
        <th scope="col">
            Номер
        </th>
        <th scope="col">
            Наименование
        </th>
    </tr>
</thead>
    @foreach(var item in Model)
    {
        <tr>
            <td scope="row">
                @item.Id
            </td>
            <td scope="row">
                @item.Name
            </td>
        </tr>
    }
</table>
